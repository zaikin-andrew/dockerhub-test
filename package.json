    "imports-loader": "0.6.5",
    "istanbul-instrumenter-loader": "0.2.0",
    "json-loader": "0.5.4",
    "node-sass": "4.1.0",
    "npm-run-all": "3.1.2",
    "parse5": "2.2.2",
    "raw-loader": "0.5.1",
    "rimraf": "2.5.4",
